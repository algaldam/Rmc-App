          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="TransaccionTotalTableAdapter" GeneratorDataComponentClassName="TransaccionTotalTableAdapter" Name="TransaccionTotal" UserDataComponentName="TransaccionTotalTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ES_SOCKSConnectionString (Settings)" DbObjectName="SELECT SL.sol_item,(SELECT ISNULL(SUM(pla_cantidad),0) FROM pmc_Plan WHERE pla_item = SL.sol_item AND pla_semana = @semana) AS CANTIDAD_PLAN, SUM(SL.sol_cant_entregada) AS CANTIDAD_ENTREGADA, ( SELECT ISNULL(SUM(sol_cant_entregada),0) FROM pmc_Solicitudes slin WHERE sol_desv = 0 AND slin.sol_item =SL.sol_item AND sol_semana = @semana) AS ENTREGA_ORIGINAL, ( SELECT ISNULL(SUM(sol_cant_entregada),0) FROM pmc_Solicitudes slin WHERE sol_desv = 1 AND slin.sol_item =SL.sol_item AND sol_semana = @semana ) AS ENTREGA_DESVIACION, ( SELECT DISTINCT pla_item_desv FROM pmc_Plan WHERE pla_item = sol_item AND pla_semana = @semana) AS DESVIACION, ( SELECT ISNULL(SUM(pla_sobrantes),0) FROM pmc_Plan WHERE pla_item = SL.sol_item AND pla_semana = @semana) AS SOBRANTES FROM pmc_Solicitudes SL WHERE sol_semana = @semana AND sol_cant_entregada &gt; 0 GROUP BY sol_item" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>	 SELECT CC.sol_item,CC.DESVIACION,CC.CANTIDAD_PLAN,SOBRANTES,CANTIDAD_ENTREGADA,CC.ENTREGA_ORIGINAL ,CC.ENTREGA_DESVIACION,
		          (CASE WHEN (CC.CANTIDAD_PLAN- CC.ENTREGA_ORIGINAL- CC.ENTREGA_DESVIACION) - CC.SOBRANTES &lt;= 0 THEN 0 
		         ELSE (CC.CANTIDAD_PLAN- CC.ENTREGA_ORIGINAL- CC.ENTREGA_DESVIACION) - CC.SOBRANTES  END) AS PENDIENTE
		        ,(CASE WHEN (CC.CANTIDAD_PLAN- CC.ENTREGA_ORIGINAL- CC.ENTREGA_DESVIACION) - CC.SOBRANTES &lt;=0 THEN  
		          (CC.ENTREGA_ORIGINAL + CC.ENTREGA_DESVIACION + CC.SOBRANTES ) - CC.CANTIDAD_PLAN ELSE 0 END) AS SOBRE_CONSUMO
