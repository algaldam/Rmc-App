          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="TransaccionEPCTableAdapter" GeneratorDataComponentClassName="TransaccionEPCTableAdapter" Name="TransaccionEPC" UserDataComponentName="TransaccionEPCTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ES_SOCKSConnectionString (Settings)" DbObjectName="SELECT PL.pla_SACA AS SACA, PL.pla_item, PL.pla_cantidad AS CANTIDAD_PLAN, SL.sol_cant_entregada AS CANTIDAD_ENTREGADA, PL.pla_estado AS ESTADO, PL.pla_sobrantes AS SOBRANTES, UPPER(FORMAT( (CASE WHEN CONVERT(TIME,SL.sol_FH_entrega) BETWEEN '07:00:01' AND '18:00:00' THEN SL.sol_FH_entrega WHEN CONVERT(TIME,SL.sol_FH_entrega) &gt;='18:00:01' AND CONVERT(TIME,SL.sol_FH_entrega) &lt;= '23:59:00' THEN SL.sol_FH_entrega WHEN CONVERT(TIME,SL.sol_FH_entrega-1) &gt;='18:00:01' AND CONVERT(TIME,SL.sol_FH_entrega) &lt;= '07:00:00' THEN SL.sol_FH_entrega-1 ELSE DATEADD(D, -1, SL.sol_FH_entrega) END), 'dddd', 'es-es')) + '-' + (CASE WHEN (DATEPART(HOUR, SL.sol_FH_entrega) &gt;= 7 AND DATEPART(HOUR,SL.sol_FH_entrega) &lt; 18) THEN 'DIURNO' ELSE 'NOCTURNO' END) AS [DIA-TURNO], PL.pla_estado FROM dbo.pmc_Plan AS PL FULL OUTER JOIN dbo.pmc_Solicitudes AS SL ON PL.pla_SACA = SL.sol_SACA AND PL.pla_semana = SL.sol_semana WHERE (PL.pla_SACA IS NOT NULL) AND (PL.pla_estilo ='EPC') AND (PL.pla_SACA LIKE 'E%') AND (PL.pla_semana = @Semana)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  @Semana AS SEMANA,SACA, pla_item AS PRODUCTO, CANTIDAD_PLAN, ISNULL(SUM(CANTIDAD_ENTREGADA), 0) AS ENTREGADO, 
                  (CASE WHEN (EPC.CANTIDAD_PLAN - ISNULL(SUM(EPC.CANTIDAD_ENTREGADA), 0)) - ISNULL(EPC.SOBRANTES, 0) 
                  &lt;= 0 THEN 0 ELSE (EPC.CANTIDAD_PLAN - ISNULL(SUM(EPC.CANTIDAD_ENTREGADA), 0)) - ISNULL(EPC.SOBRANTES, 0) END) AS PENDIENTE,ISNULL(EPC.SOBRANTES, 0)  AS SOBRANTE, 
                  (CASE WHEN (EPC.CANTIDAD_PLAN - ISNULL(SUM(EPC.CANTIDAD_ENTREGADA), 0)) - ISNULL(EPC.SOBRANTES, 0) 
                  &lt;= 0 THEN (ISNULL(SUM(EPC.CANTIDAD_ENTREGADA), 0) + ISNULL(EPC.SOBRANTES, 0)) - EPC.CANTIDAD_PLAN ELSE 0 END) AS SOBRE_CONSUMO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'DOMINGO-DIURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS DOMINGO_DIURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'DOMINGO-NOCTURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS DOMINGO_NOCTURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'LUNES-DIURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS LUNES_DIURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'LUNES-NOCTURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS LUNES_NOCTURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'MARTES-DIURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS MARTES_DIURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'MARTES-NOCTURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS MARTES_NOCTURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'MIERCOLES-DIURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS MIERCOLES_DIURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'MIERCOLES-NOCTURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS MIERCOLES_NOCTURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'JUEVES-DIURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS JUEVES_DIURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'JUEVES-NOCTURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS JUEVES_NOCTURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'VIERNES-DIURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS VIERNES_DIURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'VIERNES-NOCTURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS VIERNES_NOCTURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'SABADO-DIURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS SABADO_DIURNO, 
                  ISNULL(SUM(CASE WHEN EPC.[DIA-TURNO] = 'SABADO-NOCTURNO' THEN EPC.CANTIDAD_ENTREGADA END), 0) AS SABADO_NOCTURNO, ESTADO
FROM     (SELECT PL.pla_SACA AS SACA, PL.pla_item, PL.pla_cantidad AS CANTIDAD_PLAN, SL.sol_cant_entregada AS CANTIDAD_ENTREGADA, PL.pla_estado AS ESTADO, 
                                    PL.pla_sobrantes AS SOBRANTES, UPPER(FORMAT( (CASE
                                     WHEN CONVERT(TIME,SL.sol_FH_entrega) BETWEEN '07:00:01' AND '18:00:00' THEN SL.sol_FH_entrega
									 WHEN CONVERT(TIME,SL.sol_FH_entrega) &gt;='18:00:01' AND CONVERT(TIME,SL.sol_FH_entrega) &lt;= '23:59:00' THEN SL.sol_FH_entrega
                                     WHEN CONVERT(TIME,SL.sol_FH_entrega-1) &gt;='18:00:01' AND CONVERT(TIME,SL.sol_FH_entrega) &lt;= '07:00:00' THEN SL.sol_FH_entrega-1
                                     ELSE DATEADD(D, -1, SL.sol_FH_entrega)
                                     END), 'dddd', 'es-es')) + '-' + (CASE WHEN (DATEPART(HOUR, SL.sol_FH_entrega) &gt;= 7 AND 
                                    DATEPART(HOUR,SL.sol_FH_entrega) &lt; 18) THEN 'DIURNO' ELSE 'NOCTURNO' END) AS [DIA-TURNO], PL.pla_estado
                  FROM      dbo.pmc_Plan AS PL FULL OUTER JOIN
                                    dbo.pmc_Solicitudes AS SL ON PL.pla_SACA = SL.sol_SACA  AND PL.pla_semana = SL.sol_semana
                  WHERE   (PL.pla_SACA IS NOT NULL) AND (PL.pla_estilo ='EPC') AND (PL.pla_SACA LIKE 'E%') AND (PL.pla_semana = @Semana)) AS EPC
GROUP BY SACA, CANTIDAD_PLAN, pla_item, ESTADO,SOBRANTES</CommandText>
                    <Parameters>
