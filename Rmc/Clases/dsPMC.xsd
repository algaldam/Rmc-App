                    <CommandText> SELECT @semana AS SEMANA,CC.pla_item,CC.DESVIACION,CC.CANTIDAD_PLAN,SOBRANTES,CANTIDAD_ENTREGADA,CC.ENTREGA_ORIGINAL ,CC.ENTREGA_DESVIACION,
