  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SET LANGUAGE Spanish;
SELECT tra_id AS orden, 
	trad_flujo,
	pla_silueta,
	trad_SACA AS SACA, 
	trad_producto AS producto, 
	trad_cantidad AS cantidad,
	CASE WHEN tra_turno=0 THEN 'Diurno' ELSE 'Nocturno' END AS turno, 
	DATENAME(dw,dbo.fn_day77(trad_fecha_crea)) AS dia, 
	trad_ubicacion AS ubicacion, trad_ubicacion_cantidad AS ubicacion_cantidad, 
	U.Usr_Name AS usuario, 
	CONVERT(date,dbo.fn_day77(TD.trad_fecha_crea)) AS fecha,
	TD.trad_fecha_crea AS fechaReal

FROM Pmc_Trans AS T 

INNER JOIN Pmc_Trans_Det AS TD ON TD.trad_tra_id=T.tra_id 

INNER JOIN Pmc_Planes AS P ON P.pla_anio=T.tra_anio AND P.pla_semana=T.tra_semana 
	AND P.pla_SACA=TD.trad_SACA AND P.pla_producto=TD.trad_producto 


INNER JOIN mst_Users U ON U.Usr_Login = TD.trad_user_crea 

WHERE 


T.tra_anio = @anio AND T.tra_semana &gt;= @semana1 AND T.tra_semana&lt;=@semana2

ORDER BY orden</value>
